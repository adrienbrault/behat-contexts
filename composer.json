{
    "name": "hautelook/behat-contexts",
    "description": "Hautelook behat contexts",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "behat/behat": "~2.4",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "symfony/expression-language": "~2.4"
    },
    "suggest": {
        "guzzle/parser": "to be able to follow URI templates"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "guzzle/parser": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "Hautelook\\Behat\\Context": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
